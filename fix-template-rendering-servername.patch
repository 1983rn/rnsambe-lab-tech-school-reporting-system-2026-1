+        print(f"‚úó Test failed with error: {e}")
+        import traceback
+        traceback.print_exc()
+        assert False, f"Test failed with error: {e}"
+    
+    finally:
+        # Clean up temporary database
+        try:
+            os.unlink(test_db_path)
+        except:
+            pass

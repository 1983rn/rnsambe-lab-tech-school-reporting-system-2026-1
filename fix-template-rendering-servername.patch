+    .catch(error => {
+        alert('Error adding school: ' + error.message);
+    });
+}
+
+function toggleSchoolStatus(schoolId, currentStatus) {
+    const newStatus = currentStatus === 'active' ? 'locked' : 'active';
+    const action = newStatus === 'locked' ? 'lock' : 'unlock';

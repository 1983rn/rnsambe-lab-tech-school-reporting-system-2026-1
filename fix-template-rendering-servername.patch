+    if pkg not in reqs:
+        missing.append(pkg)
+
+if missing:
+    print('WARNING: requirements.txt is missing:', ', '.join(missing))
+else:
+    print('OK: requirements contain gunicorn, whitenoise, and Flask')
+
+# Check render.yaml presence and contents
+render_yaml = os.path.join(REPO_ROOT, 'render.yaml')
